<!DOCTYPE html>
<html>
<head>
<title><%= title %></title>
</head>
<link rel="stylesheet" href="/bower_components/tseczka-CSS-framework/dist/tseczka.css">
<link rel="stylesheet" href="/bower_components/normalize.css/normalize.css">
<body>
    <div class="nav-container nav-main nav-right">
        <div class="nav">
            <a href="/" class="nav-logo nav-title"><img src="/images/logo-white-small.png"></a> <a href="/" class="nav-title emboss"><%= site.title %></a>
            <ul>
            <% for (var i=0; i < main.length; i++) { %>
                <li>
                    <a href="/<%= main[i].slug %>">
                        <%= main[i].title %>
                    </a>
                </li>
            <% } %>
            </ul>
        </div>
    </div>
    <div class="row" id="content">
        <div class="col-8">
            <h2><%= title %></h2>
            <%= content %>
        </div>
        <div class="col-4 last">

        </div>
    </div>
</body>
</html>
